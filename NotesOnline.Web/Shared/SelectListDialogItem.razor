@using NotesOnline.Dtos;
<MudListItem OnClick="() => SList_Click()">
    @(SList is not null ? SList.Name : "List Name Unavailable")
</MudListItem>

@code {
    [Parameter]
    public SListReadDto? SList { get; set; }

    [Inject]
    private NavigationManager? NavigationManager { get; set; }

    private void SList_Click()
    {
        if (NavigationManager is not null && SList is not null)
        {
            NavigationManager.NavigateTo($"/list/{SList.Id}");
        }
    }
}
